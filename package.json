{
  "name": "hackathon-webui",
  "version": "0.0.0",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "start": "ng serve --proxy-config src/assets/proxy.conf.dev.json -c dev",
    "start:docker": "ng serve --host=0.0.0.0 --base-href /v1 --proxy-config proxy.docker.conf.json",
    "start-secure": "npm start -- --ssl --ssl-cert '~/certs/server.cert' --ssl-key '~/certs/server.key'",
    "build:prod": "ng build --base-href /v1 --cross-origin=use-credentials --source-map=false",
    "build:stage": "ng build --base-href /v1 --configuration stage",
    "test": "ng test --configuration test",
    "lint": "ng lint",
    "sonar": "sonar-scanner",
    "coverage": "ng test --watch=false --code-coverage --configuration test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/fire": "^7.3.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "firebase": "^9.8.2",
    "keycloak-angular": "^9.1.0",
    "keycloak-js": "^16.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "sonar-scanner": "^3.1.0",
    "typescript": "~4.5.2"
  }
}
